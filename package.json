{
  "name": "starship",
  "private": true,
  "engines": {
    "node": ">=12.0.0",
    "browsers": "> 0.25%"
  },
  "version": "1.0.0",
  "description": "Учебный проект в рамках Яндекс-Практикума по направлению Middle фронтенд разработчик.",
  "scripts": {
    "clear-build": "rimraf ./build",
    "assets:copy": "cp -r ./src/assets/ public",
    "assets:list": "ts-node --project ./node.tsconfig.json ./assetsCollector/index.ts ./src/assets > ./src/serviceWorker/assetsList.ts",
    "prepare": "set NODE_ENV=development && npm-run-all --print-label --parallel clear-build assets:*",
    "init": "npm run prepare && npm i && webpack --mode=development --config webpack/server.config.ts",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-client": "webpack --mode=development --config webpack/client.config.ts --watch",
    "dev:build-server": "webpack --mode=development --config webpack/server.config.ts --watch",
    "start": "set NODE_ENV=development && npm-run-all --print-label --parallel dev:*",
    "prod:build-client": "webpack --mode=production --config webpack/client.config.ts",
    "prod:build-server": "webpack --mode=production --config webpack/server.config.ts",
    "build": "set NODE_ENV=production && npm-run-all --print-label --parallel prod:*",
    "test": "jest",
    "compile": "tsc",
    "eslint": "eslint ./ --ext .ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Praktikum-Madrid/Starship.git"
  },
